{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://raw.githubusercontent.com/vladislav-zubov/json-logic-js/master/schemas/operators/logic/notEqual.json",

  "title": "Not-Equal",
  "description": "Tests not-equal, with type coercion. Requires two arguments.",

  "type": "object",
  "additionalProperties": false,
  "required": ["!="],
  "properties": {
    "!=": {
      "$ref": "https://raw.githubusercontent.com/vladislav-zubov/json-logic-js/master/schemas/common/binary-args.json"
    }
  },
  "examples": [
    { "!=": [1, 2] },
    { "!=": [1, "1"] }
  ]
}
