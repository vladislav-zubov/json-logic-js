{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://raw.githubusercontent.com/vladislav-zubov/json-logic-js/master/schemas/operators/array/map.json",

  "title": "map",
  "description": "You can use map to perform an action on every member of an array. Note, that inside the logic being used to map, var operations are relative to the array element being worked on.",
  "type": "object",
  "additionalProperties": false,
  "required": ["map"],
  "properties": {
    "map": {
      "$ref": "https://raw.githubusercontent.com/vladislav-zubov/json-logic-js/master/schemas/common/binary-args.json"
    }
  },
  "examples": [
    { "map": [
      { "var": "integers" },
      { "*": [{ "var": "" }, 2] }
    ]}
  ]
}
